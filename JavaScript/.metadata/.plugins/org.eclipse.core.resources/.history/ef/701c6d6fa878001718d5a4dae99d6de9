package Depen;

public class Carro {
	
	private String placa;
	private String cidade;
	private String modelo;
	private String marca;
	private int ano;
	private String cor;
	private int quilometragem;
	private int valor;
	
	public String getPlaca(String aString) {
		return placa;
	}
	public void setPlaca(String placa) {
		this.placa = placa;
	}
	public String getCidade(String aString) {
		return cidade;
	}
	public void setCidade(String cidade) {
		this.cidade = cidade;
	}
	public String getModelo(String aString) {
		return modelo;
	}
	public void setModelo(String modelo) {
		this.modelo = modelo;
	}
	public String getMarca(String aString) {
		return marca;
	}
	public void setMarca(String marca) {
		this.marca = marca;
	}
	public int getAno(int aint) {
		return ano;
	}
	public void setAno(int ano) {
		this.ano = ano;
	}
	public String getCor(String aString) {
		return cor;
	}
	public void setCor(String cor) {
		this.cor = cor;
	}
	public int getQuilometragem(int aint) {
		return quilometragem;
	}
	public void setQuilometragem(int quilometragem) {
		this.quilometragem = quilometragem;
	}
	public int getValor(int aint) {
		return valor;
	}
	public void setValor(int valor) {
		this.valor = valor;
	}
	

}
