package Depen;

public class CarroView {
	public void exibir(Carro carro){
		System.out.println("Carro: "+ carro.getPlaca());
		System.out.println("Carro: "+ carro.getCidade());
		System.out.println("Carro: "+ carro.getModelo());
		System.out.println("Carro: "+ carro.getMarca());
		System.out.println("Carro: "+ carro.getAno());
		System.out.println("Carro: "+ carro.getCor());
		System.out.println("Carro: "+ carro.getQuilometragem());
		System.out.println("Carro: "+ carro.getValor());
		
	}

}
