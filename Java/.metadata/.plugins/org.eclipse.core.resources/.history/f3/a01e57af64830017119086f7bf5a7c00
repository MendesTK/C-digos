package HerancaGeneralizacao;

public class SolidosGeometricos {

	private String Cone;
	private String Cilindro;
	private String Piramide;
	private String Cubo;
	
	public String getCone() {
		return Cone;
	}
	public void setCone(String cone) {
		Cone = cone;
	}
	public String getCilindro() {
		return Cilindro;
	}
	public void setCilindro(String cilindro) {
		Cilindro = cilindro;
	}
	public String getPiramide() {
		return Piramide;
	}
	public void setPiramide(String piramide) {
		Piramide = piramide;
	}
	public String getCubo() {
		return Cubo;
	}
	public void setCubo(String cubo) {
		Cubo = cubo;
	}
	
	
}
