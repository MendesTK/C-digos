package HerancaGeneralizacao;

public class Main {
	
	public static void main (String[] args){
		//Cilindro
		Cilindro c = new Cilindro();
		c.setRaio(100);
		c.setAltura(100);
		c.setId("342");
		//System.out.println(c.toString());
		System.out.println("Volume do Cilindro = "+c.calcularVolume());
		
		//Cubo
		Cilindro cb = new Cilindro();
		cb.setRaio(100);
		cb.setAltura(100);
		cb.setId("342");
		//System.out.println(c.toString());
		System.out.println("Volume do Cilindro = "+cb.calcularVolume());
		
	}

}
